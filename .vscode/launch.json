{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "디버깅",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js",
            "envFile": "${workspaceFolder}/.env.development",
            "sourceMaps": true,
            "autoAttachChildProcesses": true,
            "restart": true,
            "console": "integratedTerminal"
        }
    ]
}